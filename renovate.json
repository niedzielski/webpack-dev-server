{
  "extends": ["config:base", ":semanticCommitTypeAll(chore)"],
  "labels": ["renovate"],
  "minor": {
    "groupName": "all minor dependencies"
  },
  "patch": {
    "groupName": "all patch dependencies"
  },
  "separateMinorPatch": true,
  "rangeStrategy": "bump",
  "packageRules": [
    {
      "depTypeList": ["peerDependencies"],
      "enabled": false
    }
  ]
}
